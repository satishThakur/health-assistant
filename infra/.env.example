# Example environment variables for local development
# Copy this file to .env and fill in your values

# Garmin API credentials (get from https://developer.garmin.com/)
GARMIN_CONSUMER_KEY=your_consumer_key_here
GARMIN_CONSUMER_SECRET=your_consumer_secret_here

# LLM API keys (for meal analysis, health report parsing)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# JWT Secret (change in production!)
JWT_SECRET=your-super-secret-jwt-key-change-me

# AWS Credentials (not needed for local MinIO)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1
# AWS_S3_BUCKET=health-assistant-photos
